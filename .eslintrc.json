{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "warn",
        "no-underscore-dangle": ["error", { "allow": ["_id"] }],
        "consistent-return": "off"
    }
}
